                                 <children>
                                    <HBox alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="ALWAYS">
                                       <children>
                                              <CheckBox id="sun" fx:id="sun" mnemonicParsing="false" text="Sun" HBox.hgrow="ALWAYS" />
                                              <CheckBox id="sat" fx:id="sat" mnemonicParsing="false" text="Sat" HBox.hgrow="ALWAYS" />
                                              <CheckBox id="fri" fx:id="fri" mnemonicParsing="false" text="Fri" HBox.hgrow="ALWAYS" />
                                              <CheckBox id="thurs" fx:id="thurs" mnemonicParsing="false" text="Thurs" HBox.hgrow="ALWAYS" />
                                       </children>
                                    </HBox>
                                    <HBox alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="ALWAYS">
                                       <children>
                                              <CheckBox id="wed" fx:id="wed" mnemonicParsing="false" text="Wed" HBox.hgrow="ALWAYS" />
                                              <CheckBox id="tues" fx:id="tues" mnemonicParsing="false" text="Tues" HBox.hgrow="ALWAYS" />
                                              <CheckBox id="mon" fx:id="mon" mnemonicParsing="false" text="Mon" HBox.hgrow="ALWAYS" />
                                       </children>
                                    </HBox>
                                 </children>
                              </VBox>
                              <HBox alignment="BOTTOM_LEFT" spacing="5.0" GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.hgrow="ALWAYS" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
                                 <children>
                                    <JFXComboBox fx:id="floor" promptText="Select Floor" HBox.hgrow="ALWAYS" />
                                    <JFXComboBox fx:id="room" promptText="Select Room" HBox.hgrow="ALWAYS" />
                                 </children>
                              </HBox>
                              <HBox alignment="BOTTOM_LEFT" spacing="5.0" GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
                                 <children>
                                    <JFXComboBox fx:id="medicine" promptText="Select Medicine" HBox.hgrow="ALWAYS" />
                                    <JFXComboBox fx:id="unit" promptText="Unit" HBox.hgrow="ALWAYS" />
                                 </children>
                              </HBox>
                              <VBox alignment="CENTER_RIGHT" spacing="5.0" GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.hgrow="ALWAYS" GridPane.rowIndex="3" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
                                 <children>
                                    <HBox alignment="CENTER" spacing="5.0" VBox.vgrow="ALWAYS">
                                       <children>
                                              <DatePicker fx:id="date" promptText="Select Date" HBox.hgrow="ALWAYS" />
                                              <TextField fx:id="time" promptText="__ __ : __ __ AM/PM" HBox.hgrow="ALWAYS" />
                                       </children>
                                    </HBox>
                                        <CheckBox id="urgent" fx:id="urgent" alignment="CENTER_RIGHT" mnemonicParsing="false" text="Urgent" VBox.vgrow="ALWAYS" />
                                 </children>
                              </VBox>
                           </children>
                        </GridPane>
                        <HBox alignment="CENTER" spacing="5.0" VBox.vgrow="ALWAYS">
                           <children>
                                  <JFXButton fx:id="submitButton" alignment="CENTER" mnemonicParsing="false" onAction="#submitButton" prefHeight="75.0" prefWidth="152.0" styleClass="submitButton" text="Submit">
                                      <font>
                                          <Font name="Arial" size="18.0" />
                                      </font>
                                  </JFXButton>
                                  <JFXButton fx:id="resetButton" alignment="CENTER" mnemonicParsing="false" onAction="#resetButton" prefHeight="75.0" prefWidth="152.0" styleClass="resetButton" text="Reset">
                                      <font>
                                          <Font size="18.0" />
                                      </font>
                                  </JFXButton>
                           </children>
                        </HBox>
                     </children>
                  </VBox>
                  <VBox alignment="CENTER" HBox.hgrow="ALWAYS">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="requestFields" text="Current Requests" VBox.vgrow="ALWAYS" />
                            <TableView fx:id="medicineTable" VBox.vgrow="ALWAYS">
                                <columns>
                                    <TableColumn fx:id="tableFloor" prefWidth="-1.0" text="Floor " />
                                    <TableColumn fx:id="tableRoom" prefWidth="-1.0" text="Room" />
                                    <TableColumn fx:id="tableMedicine" prefWidth="-1.0" text="Medicine" />
                                    <TableColumn fx:id="tableQuantity" prefWidth="-1.0" text="Quantity " />
                                    <TableColumn fx:id="tableUnit" prefWidth="-1.0" text="Unit" />
                                    <TableColumn fx:id="tableDate" prefWidth="-1.0" text="Date " />
                                    <TableColumn fx:id="tableTime" prefWidth="-1.0" text="Time" />
                                    <TableColumn fx:id="tableReoccurringDays" prefWidth="-1.0" text="Reoccurring Days" />
                                    <TableColumn fx:id="tableStaff" prefWidth="-1.0" text="Staff Assigned " />
                                    <TableColumn fx:id="tableProgress" prefWidth="-1.0" text="Progress" />
                                    <TableColumn fx:id="tableNotes" prefWidth="-1.0" text="Notes" />
                                </columns>
                                <columnResizePolicy>
                                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                </columnResizePolicy>
                            </TableView>
                     </children>
                  </VBox>
               </children>
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
            </HBox>
         </children>
      </StackPane>
    </children>
</VBox>
