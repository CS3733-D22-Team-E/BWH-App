                        <StackPane minHeight="70.0" minWidth="70.0" HBox.hgrow="ALWAYS">
                            <children>
                                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="65.0" stroke="BLACK" strokeType="INSIDE" width="62.0" StackPane.alignment="CENTER" />
                                <ImageView fitHeight="65.0" fitWidth="62.0" pickOnBounds="true" preserveRatio="true" StackPane.alignment="CENTER">
                                    <image>
                                        <Image url="@icons/Language.png" />
                                    </image>
                                </ImageView>
