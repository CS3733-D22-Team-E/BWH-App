                                       <font>
                                          <Font size="15.0" />
                                       </font>
                                    </TextField>
                                    <Label styleClass="requestFields" text="Location:" GridPane.halignment="RIGHT" GridPane.hgrow="ALWAYS" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
                                       <font>
                                          <Font size="20.0" />
                                       </font>
                                    </Label>
                                    <Label styleClass="requestFields" text="Your language:" GridPane.halignment="RIGHT" GridPane.rowIndex="1">
                                       <font>
                                          <Font size="20.0" />
                                       </font>
                                    </Label>
                                    <VBox GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.hgrow="ALWAYS" GridPane.rowIndex="2" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
                                       <children>
                                          <HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                                             <children>
                                                <Label text="From ">
                                                   <font>
                                                      <Font size="20.0" />
                                                   </font>
                                                </Label>
                                                <DatePicker fx:id="startSelection" HBox.hgrow="ALWAYS">
                                                   <HBox.margin>
                                                      <Insets />
                                                   </HBox.margin>
                                                </DatePicker>
                                             </children>
                                          </HBox>
                                          <HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                                             <children>
                                                <Label prefHeight="30.0" prefWidth="52.0" text="To " HBox.hgrow="ALWAYS">
                                                   <font>
                                                      <Font size="20.0" />
                                                   </font>
                                                </Label>
                                                <DatePicker fx:id="endSelection" HBox.hgrow="ALWAYS" />
                                             </children>
                                          </HBox>
                                       </children>
                                    </VBox>
                                    <HBox alignment="CENTER_LEFT" spacing="5.0" GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.hgrow="ALWAYS" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
                                       <children>
                                          <JFXComboBox fx:id="floor" promptText="Select Floor" />
                                          <JFXComboBox fx:id="room" promptText="Select Room" />
                                       </children>
                                    </HBox>
                                    <JFXComboBox promptText="Select Language" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                                 </children>
                                 <padding>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </padding>
                              </GridPane>
                              <HBox alignment="CENTER" minHeight="10.0" minWidth="10.0" spacing="10.0" VBox.vgrow="ALWAYS">
                                 <children>
                                    <JFXButton fx:id="submitButton" maxHeight="50.0" maxWidth="150.0" minHeight="10.0" minWidth="10.0" mnemonicParsing="false" onAction="#submitButton" ripplerFill="RED" styleClass="submitButton" text="Submit" HBox.hgrow="ALWAYS" />
                                    <JFXButton fx:id="resetButton" maxHeight="50.0" maxWidth="150.0" minHeight="10.0" minWidth="10.0" onAction="#resetButton" ripplerFill="RED" styleClass="resetButton" text="Reset" HBox.hgrow="ALWAYS">
                                       <font>
                                          <Font size="17.0" />
                                       </font>
                                    </JFXButton>
                                 </children>
                              </HBox>
                           </children>
                        </VBox>
                        <VBox alignment="CENTER" HBox.hgrow="ALWAYS">
                           <children>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="requestFields" text="Current Service Requests" VBox.vgrow="ALWAYS" />
                              <TableView prefHeight="493.0" prefWidth="661.0" VBox.vgrow="ALWAYS">
                                <columns>
                                  <TableColumn prefWidth="75.0" text="Floor" />
                                  <TableColumn prefWidth="75.0" text="Room" />
                                    <TableColumn prefWidth="75.0" text="Language" />
                                    <TableColumn prefWidth="75.0" text="Interpreter" />
                                    <TableColumn prefWidth="75.0" text="Start Date" />
                                    <TableColumn prefWidth="75.0" text="End Date" />
                                    <TableColumn prefWidth="75.0" text="Other Notes" />
                                </columns>
                              </TableView>
                           </children>
                        </VBox>
                     </children>
                     <padding>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                     </padding>
                  </HBox>
               </children>
            </StackPane>
          </children>
      </VBox>
   </children>
</AnchorPane>
