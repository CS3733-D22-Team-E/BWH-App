<AnchorPane maxHeight="700.0" maxWidth="1200.0" minHeight="500.0" minWidth="700.0" stylesheets="@styles/servicerequests.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.cs3733.D22.teamE.controllers.languageInterpreterRequestController">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="10.0" layoutY="10.0" maxHeight="700.0" maxWidth="1200.0" minHeight="100.0" minWidth="100.0" stylesheets="@styles/servicerequests.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
          <children>
            <HBox maxHeight="150.0" maxWidth="1.7976931348623157E308" minHeight="100.0" minWidth="100.0" styleClass="headerBackground" VBox.vgrow="ALWAYS">
