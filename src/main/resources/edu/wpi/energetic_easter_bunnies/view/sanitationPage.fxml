<VBox xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.energetic_easter_bunnies.controllers.sanitationServiceController">
    <children>
        <HBox minHeight="-Infinity" minWidth="-Infinity" prefHeight="90.0" style="-fx-background-color: #f35959;" VBox.vgrow="NEVER">
            <children>
            <StackPane minHeight="-Infinity" minWidth="-Infinity">
               <children>
                  <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="65.0" stroke="BLACK" strokeType="INSIDE" width="62.0" />
                  <ImageView fitHeight="65.0" fitWidth="62.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@icons/sanitizer.png" />
                     </image>
                  </ImageView>
               </children>
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </StackPane>
                <VBox minHeight="-Infinity" minWidth="-Infinity" prefWidth="890.0">
                    <children>
                        <Label alignment="CENTER" text="Brigham and Women's Hospital" textFill="WHITE">
                            <font>
                                <Font name="Book Antiqua Bold" size="30.0" />
                            </font>
                        </Label>
                        <Label alignment="CENTER" text="Sanitation Services" textFill="WHITE">
                            <font>
                                <Font name="Book Antiqua Bold" size="18.0" />
                            </font>
                            <VBox.margin>
                                <Insets left="50.0" />
                            </VBox.margin>
                        </Label>
                    </children>
                </VBox>
                <Pane minHeight="-Infinity" minWidth="-Infinity" nodeOrientation="LEFT_TO_RIGHT" prefWidth="120.0" style="-fx-border-color: black;">
                    <HBox.margin>
                        <Insets />
                    </HBox.margin>
                    <children>
                        <JFXHamburger fx:id="burger" layoutX="45.0" layoutY="35.0" />
                    </children>
                </Pane>
            </children>
        </HBox>
        <StackPane>
            <children>
            <AnchorPane>
               <StackPane.margin>
                  <Insets bottom="20.0" left="40.0" right="20.0" top="60.0" />
               </StackPane.margin>
               <children>
                  <Pane layoutY="-35.0">
                     <children>
                              <VBox layoutX="173.0" layoutY="159.0">
                                  <children>
                                      <Label prefHeight="39.0" prefWidth="157.0" text="Biohazard On Site">
                                          <font>
                                              <Font name="System Bold" size="18.0" />
                                          </font>
                                      </Label>
                                      <VBox prefHeight="75.0" prefWidth="100.0" spacing="10.0">
                                          <children>
                                              <RadioButton mnemonicParsing="false" text="Yes">
                                                  <toggleGroup>
                                                      <ToggleGroup fx:id="biohazardGroup" />
                                                  </toggleGroup>
                                              </RadioButton>
                                              <RadioButton mnemonicParsing="false" selected="true" text="No" toggleGroup="$biohazardGroup" />
                                              <RadioButton mnemonicParsing="false" text="Unsure" toggleGroup="$biohazardGroup" />
                                          </children>
                                      </VBox>
                                  </children>
                              </VBox>
                              <VBox layoutY="159.0">
                                  <children>
                                      <Label prefHeight="39.0" prefWidth="157.0" text="Urgency">
                                          <font>
                                              <Font name="System Bold" size="18.0" />
                                          </font>
                                      </Label>
                                      <VBox prefHeight="75.0" prefWidth="100.0" spacing="10.0">
                                          <children>
                                              <RadioButton mnemonicParsing="false" text="Critical">
                                                  <toggleGroup>
                                                      <ToggleGroup fx:id="urgencyGroup" />
                                                  </toggleGroup>
                                              </RadioButton>
                                              <RadioButton mnemonicParsing="false" selected="true" text="Not Critical" toggleGroup="$urgencyGroup" />
                                          </children>
                                      </VBox>
                                  </children>
                              </VBox>
                              <VBox>
                                  <children>
                                      <Label prefHeight="39.0" prefWidth="398.0" text="Request Status             Assigned Staff Member">
                                          <font>
                                              <Font name="System Bold" size="18.0" />
                                          </font>
                                      </Label>
                                      <HBox spacing="15.0">
                                          <children>
                                    <JFXComboBox prefWidth="105.0">
                                       <HBox.margin>
                                          <Insets right="70.0" />
                                       </HBox.margin>
                                    </JFXComboBox>
                                              <TextField fx:id="locationField1" layoutX="10.0" layoutY="10.0" promptText="Assigned Staff...">
                                                  <font>
                                                      <Font size="14.0" />
                                                  </font>
                                              </TextField>
                                          </children>
                                      </HBox>
                                  </children>
                              </VBox>
                              <VBox layoutY="259.0" prefHeight="114.0" prefWidth="157.0">
                                  <children>
                                      <Label prefHeight="39.0" prefWidth="157.0" text="Size">
                                          <font>
                                              <Font name="System Bold" size="18.0" />
                                          </font>
                                      </Label>
                                      <VBox prefHeight="75.0" prefWidth="100.0" spacing="10.0">
                                          <children>
                                              <RadioButton mnemonicParsing="false" selected="true" text="Light">
                                                  <toggleGroup>
                                                      <ToggleGroup fx:id="sizeGroup" />
                                                  </toggleGroup>
                                              </RadioButton>
                                              <RadioButton mnemonicParsing="false" text="Medium" toggleGroup="$sizeGroup" />
                                              <RadioButton mnemonicParsing="false" text="Heavy" toggleGroup="$sizeGroup" />
                                          </children>
                                      </VBox>
                                  </children>
                              </VBox>
                              <VBox layoutX="173.0" layoutY="296.0">
                                  <children>
                                      <Label prefHeight="39.0" prefWidth="152.0" text="Additional Notes">
                                          <font>
                                              <Font name="System Bold" size="18.0" />
                                          </font>
                                      </Label>
                                      <TextField fx:id="notes" promptText="Note...">
                                          <font>
                                              <Font size="14.0" />
                                          </font>
                                      </TextField>
                                  </children>
                              </VBox>
                              <TableView layoutX="398.0" layoutY="35.0" prefHeight="400.0" prefWidth="631.0">
                                  <columns>
                                      <TableColumn prefWidth="75.0" text="Request ID" />
                                      <TableColumn prefWidth="65.0" text="Status" />
                                      <TableColumn prefWidth="101.0" text="Assigned Staff" />
                                      <TableColumn prefWidth="63.0" text="Urgency" />
                                      <TableColumn prefWidth="76.0" text="Biohazard" />
                                      <TableColumn prefWidth="63.0" text="Size" />
                                      <TableColumn prefWidth="97.0" text="Floor ID" />
                              <TableColumn prefWidth="90.0" text="Room ID" />
                                  </columns>
                              </TableView>
                              <VBox layoutY="81.0">
                                  <children>
                                      <Label prefHeight="39.0" prefWidth="398.0" text="Floor Number              Room Number">
                                          <font>
                                              <Font name="System Bold" size="18.0" />
                                          </font>
                                      </Label>
                                      <HBox spacing="15.0">
                                 <children>
                                    <JFXComboBox prefWidth="105.0">
                                       <HBox.margin>
                                          <Insets right="50.0" />
                                       </HBox.margin>
                                    </JFXComboBox>
                                    <JFXComboBox layoutX="60.0" layoutY="10.0" prefWidth="105.0">
                                       <HBox.margin>
                                          <Insets left="20.0" />
                                       </HBox.margin>
                                    </JFXComboBox>
                                 </children>
                                      </HBox>
                                  </children>
                              </VBox>
                        <JFXButton buttonType="RAISED" layoutY="382.0" onAction="#submitButton" prefHeight="39.0" prefWidth="105.0" ripplerFill="RED" style="-fx-background-color: #f1f1f1;" text="Submit" />
                        <JFXButton buttonType="RAISED" layoutX="173.0" layoutY="382.0" onAction="#resetFields" prefHeight="39.0" prefWidth="105.0" ripplerFill="RED" style="-fx-background-color: #f1f1f1;" text="Reset Fields" />
                     </children>
                  </Pane>
               </children>
            </AnchorPane>
            <JFXDrawer fx:id="drawer" defaultDrawerSize="120.0" direction="RIGHT" style="-fx-background-color: transparent;" />
            </children>
        </StackPane>
    </children>
</VBox>
