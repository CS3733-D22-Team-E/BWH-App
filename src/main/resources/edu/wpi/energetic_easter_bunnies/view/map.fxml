            <JFXComboBox fx:id="nodeTypeDropdown" layoutX="165.0" layoutY="110.0" onAction="#nodeTypeDropdown" promptText="Select Type" />
            <JFXComboBox fx:id="floorDropdown" layoutX="28.0" layoutY="110.0" onAction="#floorDropdown" promptText="Select Floor" />
            </children>
        </AnchorPane>
